}
.logo {
    background: url(images/logo.png) no-repeat;
    width: 187px;
    height: 68px;
    display: block;
    position: absolute;
    top: 32px;
    text-indent: -9999px;
    text-align: left;
    z-index: 200;
}
.breadcrumb {
    padding-bottom: 15px;
    padding-top: 20px;
    border-bottom: 1px solid #efefef;
    margin-bottom: 25px;
}
.maincol .meta {
    padding-bottom: 20px;
    display: block;
}
/*
3.2. LAYOUT: Homepage
============
 */
/* Scaling transforms */
.rsSlide img {
    opacity: 0.35;
    -webkit-transition: all 0.3s ease-out;
    -moz-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
}
.rsActiveSlide img {
    opacity: 1;
    -webkit-transform: scale(1);  
    -moz-transform: scale(1); 
    -ms-transform: scale(1);
    -o-transform: scale(1);
    transform: scale(1);
}
.rsOverflow {
    cursor: default !important;
}
.rsABlock {
    text-align: center;
    top: 160px;
    width: 942px;
    font-size: 34px;
}
.txtCent {
    top: 200px;
    font-size: 16px;
}
.row1 .internal > div,
.row2 .internal > div,
.row3 .internal > div {
    float: left;
}
.row1 {
